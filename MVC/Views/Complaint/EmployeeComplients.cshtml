@model IEnumerable<MVC.Models.ComplientBox>

@{
    ViewData["Title"] = "EmployeeComplients";
    Layout = "~/Views/Shared/_HomeLayoutEmployee.cshtml";
}

<h1>Employee Complaints</h1>

<p>
    <a asp-action="create">create new</a>
</p>
<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.EmployeeId)
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeId)
            </td>
            
            <td>
                    @Html.ActionLink("ClickOnYourId", "GetByEmployeeId", new { id=item.EmployeeId }) 
            </td>
        </tr>
}
    </tbody>
</table>
